	ad_material.descricao,
	ad_material.unidade,
	ad_estoque.estoquemin,
	ad_estoque.estoque,
	ad_estoque.valortotal,
	(select nome from ad_uo where iduo=$P{pIdUo}) as uo

FROM (ad_estoque INNER JOIN ad_material ON ((ad_estoque.idmaterial=ad_material.idmaterial)))
