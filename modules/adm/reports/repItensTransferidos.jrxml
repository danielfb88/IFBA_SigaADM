	<queryString>
		<![CDATA[select s.nomesetor,
     i.descricao,
     TO_CHAR(i.DATAAQUISICAO,'DD/MM/YYYY') as data,
     i.valor,
     i.iditempatrimonio,
     s_1.nomesetor as setororigem

from ad_itempatrimonio as i inner join cm_setor as s on (i.idsetor = s.idsetor)
inner join ad_movimentopat as m on (m.iditempatrimonio = i.iditempatrimonio)
inner join cm_setor as s_1 on (m.idsetororigem = s_1.idsetor)


where i.idsetor = $P{pIdSetor}
    and i.ativo ilike('S')
    and i.iditempatrimonio in ($P!{itens})
    and m.tipomovimento = '2'
    and m.data = to_date($P{dataTransferencia},'DD/MM/YYYY')

order by idmovimentacao desc limit $P{numItens}]]>
	</queryString>
